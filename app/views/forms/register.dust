{>layout/}
{<content}
<div class="form">
   <h1>Registration</h1>
   <form action="/register" method="post">
      <input type="text" name="display">
      <input type="text" name="email">
      <input type="password" name="password">
      <input type="password" name="passConfirm">
      {?err}
      <div>{err.msg}</div>
      {/err}
      <input type="submit" value="register">
   </form>
</div>
{/content}
