{
  "name": "linkwrapper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch-player": "watchify public/js/player/index.js -o public/js/_player.js",
    "watch-site": "watchify public/js/index.js -o public/js/_site.js",
    "start": "npm run watch-player & npm run watch-site & nodemon -e js,css,dust ./server.js",
    "test": "cd test && ./runTests.sh",
    "build-player": "browserify public/js/player/index.js -o public/js/_player.js --debug --verbose",
    "build-site": "browserify public/js/index.js -o public/js/_site.js --debug --verbose",
    "build": "npm run build-player && npm run build-site"
  },
  "engines": {
    "node": "0.11.14"
  },
  "dependencies": {
    "adaro": "0.1.x",
    "bcrypt": "0.8.x",
    "body-parser": "1.10.x",
    "browserify": "^8.1.0",
    "bunyan": "^1.3.2",
    "checky": ">=1.3.x",
    "cookies": "^0.5.0",
    "dustjs-helpers": "1.5.x",
    "dustjs-linkedin": "2.5.x",
    "elman": ">=0.3.4",
    "express": "4.11.x",
    "link-id": ">=1.0.0",
    "mailgun-js": "^0.6.7",
    "mongodb": "1.4.x",
    "morgan": "1.5.x",
    "node-uuid": "^1.4.2",
    "passport": "0.2.x",
    "passport-facebook": "1.0.x",
    "passport-google-oauth": "0.1.x",
    "passport-local": "1.0.x",
    "premailer-api": "^1.0.3",
    "redis": "0.12.x",
    "redis-limiter": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "1.3.x",
    "watchify": "2.2.x",
    "tape": "3.4.x",
    "supertest": "0.15.x"
  }
}
